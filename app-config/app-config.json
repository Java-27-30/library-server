{
  "port":3500,
  "skipRoutes" : [
    "POST/accounts",
    "POST/accounts/login",
    "GET/api/books"
  ],
  "pathRoles": {
    "GET/accounts/reader_id" : ["user", "admin","premium"],
    "PATCH/accounts/password" : ["user", "premium"],
    "DELETE/accounts" : ["super"],
    "PATCH/accounts" : ["user","admin", "premium"],
    "PUT/accounts/roles" : ["super"],
    "GET/api/books/book_id" : ["user", "admin", "librarian", "premium"],
    "GET/api/books/genre" : ["user", "librarian", "premium"],
    "GET/api/books/gen_st" : ["librarian"],
    "POST/api/books":["admin"],
    "DELETE/api/books":["admin"],
    "PATCH/api/books/pickup":["librarian"],
    "PATCH/api/books/return":["librarian"],
    "GET/api/books/books_reader":["admin", "librarian"]
  },
  "checkIdRoutes": [
    "GET/accounts/reader_id",
    "PATCH/accounts/password",
    "PATCH/accounts"
  ],
  "timeWindowMs": 60000,
  "requestLimit": 3
}